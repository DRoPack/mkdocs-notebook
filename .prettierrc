{
  "singleQuote": true,
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "useTabs": false,
  "tabWidth": 2,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.md",
      "options": { "tabWidth": 1 }
    },
    {
      "files": ["*.yml", "*.yaml"],
      "options": { "tabWidth": 2, "singleQuote": false }
    }
  ]
}
